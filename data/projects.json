      }
    ],
    "awards": [
      {
        "name": "Shortlisted Master of Architectural Design Awards for Excellence | 2024 Semester 1 | RMIT University",
        "url": "https://www.instagram.com/p/C8vhpFsyVCO/?img_index=3"
      }
    ],
